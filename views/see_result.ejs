<%- include("../partials/header-user") -%>
<%- include("../partials/user-nav") -%>
	<div class="main-panel">
		<div class="content-wrapper">
			<div class="row">
				<div class="col-md-12 stretch-card">
					<div class="card">
						<div class="card-body">
							<p class="card-title text-center">Result<hr></p>
							<% if (answer) { %>
							<% answer.forEach(function(ans){ %>
							<% if(ans.marks == 0){  %>
							<p class="lead">Result not published yet</p>
							<% }else if(ans.marks >= 33){ %>
							<div class="card m-auto" style="width: 18rem;">
								<img src="/img/passed.png" class="card-img-top" alt="...">
								<div class="card-body">
									<h4 class="card-title">Marks: <%= ans.marks %></h4>
								</div>
							</div>
							<% }else if (ans.marks  < 33 ){ %>
							<div class="card m-auto" style="width: 18rem;">
								<img src="/img/failed.png">
								<h5 class="card-title">Marks: <%= ans.marks %></h5>
							</div>
						</div>
						<% } %>
						
						<% }) %>
						<% } %>
					</div>
				</div>
			</div>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
			<script type="text/javascript">
				
				var  cls = JSON.parse('<%- JSON.stringify(answer) %>');
			console.log(cls)
			</script>
			<%- include("../partials/footer-user") -%>