<%- include("../partials/header-user") -%>
<div class="container-fluid page-body-wrapper">
	<nav class="sidebar sidebar-offcanvas" id="sidebar">
		<ul class="nav">
			<li class="nav-item">
				<a class="nav-link" href="/user/dashboard">
					<i class="mdi mdi-home menu-icon"></i>
					<span class="menu-title">Dashboard</span>
				</a>
			</li>
		</ul>
	</nav>
	<!-- partial -->
	<div class="main-panel">
		<div class="content-wrapper">
			<div class="row">
				<div class="col-md-12 stretch-card">
					<div class="card">
						<div class="card-body">
							<p class="card-title text-center">Result<hr></p>
									<% if (answer) { %>
				<% answer.forEach(function(ans){ %>
					
				  <% if(ans.marks == 0){  %>
					<p class="lead">Result not published yet</p>
				  <% }else if(ans.marks >= 33){ %>
					<div class="card m-auto" style="width: 18rem;">
						<img src="/img/passed.png" class="card-img-top" alt="...">
						<div class="card-body">
							<h4 class="card-title">Marks: <%= ans.marks %></h4>
						</div>
					</div>
				  <% }else if (ans.marks  < 33 ){ %>
					<div class="card m-auto" style="width: 18rem;">
						<img src="/img/failed.png">
							<h5 class="card-title">Marks: <%= ans.marks %></h5>
						</div>
					</div>
				  <% } %>
					
				<% }) %>




					<% } %>
 						
